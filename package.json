    "autoprefixer": "10.4.19",
    "electron": "31.2.0",
    "electron-builder": "24.13.3",
    "electron-vite": "2.3.0",
    "eslint": "8.57.0",
    "eslint-plugin-react": "7.34.3",
    "postcss": "8.4.39",
    "prettier": "3.3.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "rimraf": "6.0.1",
    "tailwindcss": "3.4.4",
    "typescript": "5.5.3",
    "vite": "5.3.3"
  }
